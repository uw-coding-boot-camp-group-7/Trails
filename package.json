    "sequelize": "^4.41.2"
  },
  "devDependencies": {
    "bulma": "^0.7.4",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.13.7"
  },
  "directories": {
    "test": "test"
  },
